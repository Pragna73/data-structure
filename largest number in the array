#include <stdio.h>
int main()
{
    int array[10];
    int n = 10; 
int largest;
    for (int i = 0; i < n; i++) {
        array[i] = i + 1; // Assign values 1 to 10
    }
    largest = array[0];
    for (int i = 1; i < n; i++) {
        if (array[i] > largest) {
            largest = array[i];
        }
    }
    printf("Largest element in the array: %d\n", largest);
    return 0;
}
