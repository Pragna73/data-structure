#include <stdio.h>
int main() 
{
    int array[10];
    int sum = 0;
    int n = 10;
    float average;
    for (int i = 0; i < n; i++)
{
        array[i] = i + 1;
    }
    for (int i = 0; i < n; i++) {
        sum += array[i];
    }
    average = (float)sum / n;
    printf("Sum of array elements: %d\n", sum);
    printf("Average of array elements: %.2f\n", average);
    return 0;
}
