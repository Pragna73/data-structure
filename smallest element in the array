#include <stdio.h>
int main()
{
    int array[10];
    int n = 10; // Size of the array
    int smallest;
    for (int i = 0; i < n; i++) {
        array[i] = i + 1; 
    }
    smallest = array[0];
    for (int i = 1; i < n; i++) {
        if (array[i] < smallest) {
            smallest = array[i];
        }
    }
    printf("Smallest element in the array: %d\n", smallest);
    return 0;
}
